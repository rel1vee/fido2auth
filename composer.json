{
    "name": "mzes/fido2auth",
    "description": "Advanced security with FIDO2-based multi-factor authentication.",
    "type": "prestashop-module",
    "license": "AFL-3.0",
    "authors": [
        {
            "name": "Muh. Zaki Erbai Syas",
            "email": "muhzakierbaisyas@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "web-auth/webauthn-lib": "^4.9",
        "web-auth/cose-lib": "^4.3",
        "ramsey/uuid": "^4.7",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "psr/http-message": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "PrestaShop\\Module\\Fido2Auth\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "prepend-autoloader": false,
        "sort-packages": true
    }
}